  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "preload": {
      "paths": [
        "app",
        "config",
        "routes"
      ],
      "exclude": [
        "database",
        "tests"
      ],
      "no-status-check": false
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "ComdexxSolutionsLLC\\": "vendor_internal/laravel-scout-mysql-driver/src/"
    },
    "classmap": [
      "app/Macros",
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan ide-helper:model -RW",
      "@php artisan dcas:clear-project-files",
      "@php artisan dcas:make-tree-output",
      "@php artisan dcas:make-phpdev-reports disable",
      "@php artisan debugbar:clear",
      "@php artisan dusk:update",
      "@php artisan laroute:generate"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
